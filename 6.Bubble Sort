# bubble sort

def bubble_sort(arr):
    for i in range(1,len(arr)):       
        for j in range(i-1,len(arr)):
            if arr[j-1] > arr[j]:
                arr[j-1],arr[j] = arr[j],arr[j-1]               
            else:
                continue

    return arr

if __name__ == "__main__":
    list1 = [5,45,2,6,3,89,10]
    print(bubble_sort(list1))
